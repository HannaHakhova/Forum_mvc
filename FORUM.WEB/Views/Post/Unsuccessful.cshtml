
@{
    ViewBag.Title = "Unsuccessful";
}

<h4>Unfortunately, something went wrong and your skill set has not been updated. Please, try again later.</h4>
<div class="row">
    <div class="col-md-3">
        <p><a class="btn btn-default" href="@Url.Action("Index", "Topic")">Back to topics</a></p>
    </div>
</div>

@if (Request.IsAuthenticated && User.IsInRole("Administrator"))
{
    <p>@ViewBag.Message</p>
}


